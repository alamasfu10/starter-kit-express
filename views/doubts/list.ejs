<h1 class="headline">Doubts list</h1>

<section class="section-doubts">
  <div class="section-doubts__doubts">
    <% for(var i=0; i<doubts.length; i++) {%>
      <div class="doubt">
        <div class="doubt__title">
          <%= doubts[i].title %>
        </div>
        <div class="doubt__description">
          <%= doubts[i].description %>
        </div>
        <div class="doubt__footer">
          <div class="doubt__date">23/02/2018</div>
          <div class="doubt__delete">
            <a href="/doubts/delete/<%= doubts[i].id %>">Borrar</a>
          </div>
        </div>
      </div>
    <% } %>
  </div>
</section>
